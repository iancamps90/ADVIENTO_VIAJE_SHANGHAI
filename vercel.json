{
  "version": 2,
  "builds": [
    {
      "src": "adeviento_web.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "adeviento_web.py"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
