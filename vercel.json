{
  "version": 2,
  "builds": [
    {
      "src": "adeviento_web.py",
      "use": "@vercel/python"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "adeviento_web.py"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ],
  "functions": {
    "adeviento_web.py": {
      "runtime": "python3.9"
    }
  }
}
